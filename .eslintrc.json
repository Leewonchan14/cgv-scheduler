{
  "extends": [
    "next",
    "prettier",
    "eslint:recommended",
    "next/typescript",
    "plugin:@tanstack/query/recommended"
  ],
  "rules": {
    "@typescript-eslint/no-empty-object-type": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn", // or "error"
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_"
      }
    ],
    "no-empty-pattern": "off"
  }
}
